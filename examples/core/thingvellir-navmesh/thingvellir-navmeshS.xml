    <ORCA tau="3" tauObst="0.15" />
  </AgentProfile>
  <AgentProfile name="group2">
    <Common max_angle_vel="360" max_neighbors="10" obstacleSet="1" neighbor_dist="5" r="0.19" class="1" pref_speed="0.8" max_speed="1.8" max_accel="4" />
    <ORCA tau="3" tauObst="0.15" />
  </AgentProfile>
  <AgentGroup>
    <ProfileSelector type="const" name="group1" />
    <StateSelector type="const" name="Walk1" />
    <Generator type="explicit">
      <Agent p_x="64" p_y="-165" />
    </Generator>
  </AgentGroup>
</Experiment>